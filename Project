CREATE TABLE  "DEPARTMENTS"
   (
	"DEPARTMENT_NAME" VARCHAR2(30) CONSTRAINT "DEPT_NAME_NN" NOT NULL ENABLE,
	"BUILDING" VARCHAR2(25),    
	"MANAGER_NAME" VARCHAR(20),
	CONSTRAINT "DEPT_NAME_PK" PRIMARY KEY ("DEPARTMENT_NAME")
	USING INDEX  ENABLE
   );
 
