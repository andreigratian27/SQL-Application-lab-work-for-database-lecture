CREATE TABLE "STUDENTS" (
"STUDENT_ID" NUMBER(6,0) CONSTRAINT "STUDENT_ID_NN" NOT NULL ENABLE,
"ADVISOR_ID" NUMBER(5,0) CONSTRAINT "ADVISOR_ID_NN" NOT NULL ENABLE,
"STUDENT_NAME" VARCHAR2(30) CONSTRAINT "STUDENT_NAME_NN" NOT NULL ENABLE,
"CREDIT_POINTS" NUMBER(3,0) ,
"STUDENT_DEPARTMENT" VARCHAR2(30) CONSTRAINT "STUDENT_DEPARTMENT_NN" NOT NULL ENABLE,
CONSTRAINT "STUDENT_ID_PK" PRIMARY KEY ("STUDENT_ID") USING INDEX  ENABLE
);

ALTER TABLE  "STUDENTS" ADD CONSTRAINT "ADVISOR_ID_FK" FOREIGN KEY ("ADVISOR_ID")
	REFERENCES  "INSTRUCTORS" ("ID_INSTRUCTOR") ENABLE;
ALTER TABLE  "STUDENTS" ADD CONSTRAINT "STUDENT_DEPARTMENT_FK" FOREIGN KEY ("STUDENT_DEPARTMENT")
	REFERENCES  "DEPARTMENTS" ("DEPARTMENT_NAME") ENABLE;


