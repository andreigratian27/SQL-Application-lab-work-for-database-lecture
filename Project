DROP TABLE CLASSROOM;
DROP TABLE DEPARTMENTS;

CREATE TABLE  "CLASSROOM"
   (
    "BUILDING" VARCHAR2(20) CONSTRAINT "BUILDING_NN" NOT NULL ENABLE,
    "ROOM_NUMBER" VARCHAR2(7) CONSTRAINT "ROOM_NUMBER_NN" NOT NULL ENABLE,
    "CAPACITY" NUMBER(4,0)
    CONSTRAINT "CLASSROOM_PK" PRIMARY KEY ("BUILDING","ROOM_NUMBER") 
    USING INDEX  ENABLE
   );
   
